<html>
<body>
<cript lenguaje="javascript" type="text/javascript">
var number
function calculoIMC (){
var peso=number(prompt("igrese peso"))
var altura=number(prompt("igrese altura"))
var IMC= peso/(altura*altura)
if (IMC>30)
alert("alto gordo")
else if (IMC>25)
alert("ta gordo")
else if (IMC>17.9)
alert(" diez puntos")
else if (IMC<17.9)
alert("re flako")
}
</script>
<input type="button" onclick="calculoIMC()" value="calcularIMC"/>
</body>
</html>
